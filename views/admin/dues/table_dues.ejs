<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <!-- Button trigger modal -->
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Invoice</th>
                        <th>Date</th>
                        <th>Name Member</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let i=0; i < dues.length; i++) { %>
                        <tr>
                            <td>
                                <%= i + 1 %>
                            </td>
                            <td>#<%= dues[i].invoice %></td>
                            <td>
                                <%= dues[i].duesStartDate.toLocaleString().split(',')[0] %>
                            </td>
                            <td>
                                <%= dues[i].memberId.firstName %>
                                    <%= dues[i].memberId.lastName %>
                            </td>
                            <td>
                                <%= dues[i].payments.status %>
                            </td>
                            <td>
                                <a href="/admin/dues/<%= dues[i].id %>"
                                    class="btn btn-info btn-sm"><i class="fas fa-eye"></i></a>

                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>